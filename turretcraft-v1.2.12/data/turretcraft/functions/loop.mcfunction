tag @a[tag=!TC_Setup] add TC_Setup
execute at @a store result score @p TC_UUID0 run data get entity @p UUID[0]
execute at @a store result score @p TC_UUID1 run data get entity @p UUID[1]
execute at @a store result score @p TC_UUID2 run data get entity @p UUID[2]
scoreboard players set @e[tag=TurretLimit,type=armor_stand,scores={TC_TurretRot=..-2}] TC_TurretRot 0
scoreboard players reset @a TC_Turret
scoreboard players reset @a TC_Target
scoreboard players set @a[scores={TC_RightClick=1..}] TC_RightClick 0
scoreboard players set @a[scores={TC_Shifting=1..}] TC_Shifting 0
scoreboard players enable @a TC_Turret
scoreboard players enable @a TC_Target
scoreboard players enable @a TC_Frequency
scoreboard players set @e[type=armor_stand,tag=TC] TC_TurretRotS -90

#warcriminal advancement
execute at @a[nbt={SelectedItem:{id:"minecraft:carrot_on_a_stick",tag:{isGun:1b}}},scores={TC_KillVillager=1..}] run scoreboard players operation @p TC_TurretCon += @p TC_KillVillager
scoreboard players reset @a[scores={TC_KillVillager=1..}] TC_KillVillager
advancement grant @a[scores={TC_TurretCon=100..}] only turretcraft:warcriminal

#robotarmy advancement
advancement grant @a[scores={TC_MaxTurrets=4..}] only turretcraft:robotarmy
